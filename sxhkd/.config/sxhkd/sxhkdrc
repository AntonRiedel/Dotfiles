##Keybindings for programs
super + Return
	$TERMINAL
super + {_,shift} + b
	{$BROWSER,$BROWSER2}
super + {_,shift} + n
	{$TERMINAL -e $FILE,nextsync.sh}
super + {_,shift} + e
	{$TERMINAL -e neomutt,mailsync}
super + v
	$TERMINAL -e newsboat
super + o
    xournalpp

##Bezel Keybindings
XF86TaskPane
    thinkpad-rotate flip
#take a screenshot rather than rotating the screen by 90
XF86RotateWindows
    flameshot gui
    #thinkpad-rotate left

##Media Keybindings
super + {_,shift} + a
	{$TERMINAL -e pulsemixer,sound.sh mute}
XF86AudioMute
    sound.sh mut
XF86Audio{LowerVolume,RaiseVolume}
	{sound.sh down 10,sound.sh up 10}
super + {_,shift} + p
	{sound.sh down 10,sound.sh up 10}

##Menu Keybindings
super + x
	fin.sh
super + {_,shift} + r
    {dmenu_run -i -F -p "Run: ",screen.sh}
super + g
	passmenu
