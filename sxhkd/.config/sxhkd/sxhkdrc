##Keybindings for programs
super + Return
	$TERMINAL
super + {_,shift} + b
	{$BROWSER,$BROWSER2}
super + {_,shift} + n
	{$TERMINAL -e $FILE,$FILE2}
super + v
	$TERMINAL -e newsboat

##Media Keybindings
super + {_,shift} + a
	{$TERMINAL -e pulsemixer,sound.sh mut}
super + {_,shift} + p
	{sound.sh up 10,sound.sh down 10}

##Email Keybinding
super + {_,shift} + e
	{$TERMINAL -e neomutt,$TERMINAL -e mailsync}

##Menu Keybindings
super + x
	fin.sh
super + {_,shift} + r
	{dmenu_run,screen.sh}
super + g
	passmenu
