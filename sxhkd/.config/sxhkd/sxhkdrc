##Keybindings for programs
super +  Return
	$TERMINAL
super + {_,shift} + b
	{$BROWSER,$BROWSER2}
super + {_,shift} + n
	{$TERMINAL -e $FILE,$HOME/.local/bin/nextcloud}
super + {_,shift} + v
	{$TERMINAL -e newsboat,$TERMINAL -e calcurse}

##Media Keybindings
super + {_,shift} + a
	{$TERMINAL -e pulsemixer,sound.sh mut}
super + {_,shift} + p
	{sound.sh up 10,sound.sh down 10}

##Email Keybinding
super + {_,shift} + e
	{$TERMINAL -e neomutt,$TERMINAL -e mailsync}

##Menu Keybindings
super + x
	fin.sh
super + s
	dmenu_run
super + g
	passmenu
